<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>launcher</servlet-name>
		<servlet-class>com.hyk.proxy.gae.server.core.Launcher</servlet-class>
		<init-param>
			<param-name>remoteserver.rpc.compressor.type</param-name>
			<param-value>gz</param-value>
		</init-param>
		<init-param>
			<param-name>remoteserver.rpc.compressor.trigger</param-name>
			<param-value>256</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>HttpProxy</servlet-name>
		<servlet-class>com.hyk.proxy.gae.server.core.servlet.HttpProxyServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>XmppProxy</servlet-name>
		<servlet-class>com.hyk.proxy.gae.server.core.servlet.XmppProxyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>XmppProxy</servlet-name>
		<url-pattern>/_ah/xmpp/message/chat/</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>HttpProxy</servlet-name>
		<url-pattern>/fetchproxy</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
