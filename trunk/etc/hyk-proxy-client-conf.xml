<?xml version="1.0" encoding="utf-8"?>
<Configure>
	<localserver host="localhost" port="48101" />

    <!-- Can config more than 1 hyk-proxy-server -->
	<hyk-proxy-server appid="hykserver" user="root" passwd="hello"/>
	<!--  <hyk-proxy-server appid=""  user="" passwd=""/> -->
    
    <!-- 1: HTTP Direct 2:XMPP Enable  -->
    <connectionMode>1</connectionMode>
    
    <!-- Can config more than 1 account -->
    <XMPPAccount user="" passwd="" />
    <!--<XMPPAccount user="" passwd="" serverHost="" serverPort="" oldSSLEnable="" /> -->

    <httpUpStreamEncrypter>se1</httpUpStreamEncrypter>

	<threadPoolSize>25</threadPoolSize>
	<httpConnectionPoolSize>7</httpConnectionPoolSize>
	<RPCTimeOut>120</RPCTimeOut>
	<simpleURLEnable>false</simpleURLEnable>
	<compressor>lzf</compressor>
	<fetchLimitSize>250000</fetchLimitSize>
	<!-- for big file(>=1m) & video -->
	<maxFetcherNumber>3</maxFetcherNumber>

	<localProxy>
	    <host></host> 
		<!--
			<port>80</port> 
			<user>xxx</user>
			<passwd>yyy</passwd>
		-->
	</localProxy>

	<defaultLocalProxy>
		<host>www.google.com.hk</host>
	</defaultLocalProxy>
</Configure>