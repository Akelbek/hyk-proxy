<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	android:versionCode="1" android:versionName="0.9.5" package="org.hyk.proxy.android">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".activity.LaunchActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
				<category android:name="android.intent.category.BROWSABLE" />
			</intent-filter>
		</activity>
		<activity android:name=".activity.ConfigurationActivity"></activity>
		
		<service android:enabled="true" android:name=".service.ProxyService" android:exported="false">
			<intent-filter>
	             <action android:name="org.hyk.proxy.android.service.ProxyService" />
	             <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
		</service>
		


	</application>
</manifest>